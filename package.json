{
	"name": "quick-developing-vscode",
	"displayName": "Quick Developing",
	"description": "An extension for \"Quick Developing\" system",
	"publisher": "Simutis",
	"version": "0.0.1",
	
	"engines": {
		"vscode": "^1.0.0"
	},
	"author": {
		"email": "julius.simutis@gmail.com",
		"name": "Julius Simas Simutis",
		"url": "https://simofy.github.io/"
	 },  
	"categories": [
		"Other"
	],
	"repository": {
		"url": "https://github.com/Simofy/quick-developing-vscode"
	},
	"activationEvents": [
		"onCommand:extension.quick.developing.connect"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.quick.developing.connect",
				"title": "Open connection",
				"category": "Quick"
			},
			{
				"command": "extension.quick.developing.disconnect",
				"title": "Close connection",
				"category": "Quick"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.0.0",
		"glob": "^7.1.5",
		"mocha": "^10.2.0",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"@types/parse5": "^5.0.2",
		"@types/string-similarity": "^3.0.0",
		"@types/ws": "^6.0.4",
		"hyntax": "^1.1.2",
		"parse5": "^5.1.1",
		"string-similarity": "^3.0.0",
		"ws": "^7.2.1"
	}
}
